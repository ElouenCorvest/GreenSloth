@article{reiserArabidopsisInformationResource2024a,
  title = {The {{Arabidopsis Information Resource}} in 2024},
  author = {Reiser, Leonore and Bakker, Erica and Subramaniam, Sabarinath and Chen, Xingguo and Sawant, Swapnil and Khosa, Kartik and Prithvi, Trilok and Berardini, Tanya Z},
