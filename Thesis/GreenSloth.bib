@article{urbanoEnhancingBiodiversityConservation2024,
  title = {Enhancing Biodiversity Conservation and Monitoring in Protected Areas through Efficient Data Management},
  author = {Urbano, Ferdinando and Viterbi, Ramona and Pedrotti, Luca and Vettorazzo, Enrico and Movalli, Cristina and Corlatti, Luca},
  year = 2024,
  month = jan,
  journal = {Environmental Monitoring and Assessment},
  volume = {196},
