@article{baiDynamicKnowledgeGraph2024,
  title = {A Dynamic Knowledge Graph Approach to Distributed Self-Driving Laboratories},
  author = {Bai, Jiaru and Mosbach, Sebastian and Taylor, Connor J. and Karan, Dogancan and Lee, Kok Foong and Rihm, Simon D. and Akroyd, Jethro and Lapkin, Alexei A. and Kraft, Markus},
