@article{clarkeRummageneMassiveMining2024,
  title = {Rummagene: Massive Mining of Gene Sets from Supporting Materials of Biomedical Research Publications},
  shorttitle = {Rummagene},
  author = {Clarke, Daniel J. B. and Marino, Giacomo B. and Deng, Eden Z. and Xie, Zhuorui and Evangelista, John Erol and Ma'ayan, Avi},
