@article{maureaudFISHGLOB_dataIntegratedDataset2024,
  title = {{{FISHGLOB}}\_data: An Integrated Dataset of Fish Biodiversity Sampled with Scientific Bottom-Trawl Surveys},
  shorttitle = {{{FISHGLOB}}\_data},
  author = {Maureaud, Aurore A. and {Palacios-Abrantes}, Juliano and Kitchel, Zo{\"e} and Mannocci, Laura and Pinsky, Malin L. and Fredston, Alexa and Beukhof, Esther and Forrest, Daniel L. and Frelat, Romain and Palomares, Maria L. D. and Pecuchet, Laurene and Thorson, James T. and Van Denderen, P. Dani{\"e}l and M{\'e}rigot, Bastien},
