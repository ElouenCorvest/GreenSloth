  title = {Understanding Science Data Literacy: A Conceptual Framework and Assessment Tool for College Students Majoring in {{STEM}}},
  shorttitle = {Understanding Science Data Literacy},
  author = {Qiao, Cuilan and Chen, Yuqing and Guo, Qing and Yu, Yunwei},
