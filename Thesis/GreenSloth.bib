  author = {Giang, Amanda and Edwards, Morgan R. and Fletcher, Sarah M. and {Gardner-Frolick}, Rivkah and Gryba, Rowenna and Mathias, Jean-Denis and {Venier-Cambron}, Camille and Anderies, John M. and Berglund, Emily and Carley, Sanya and Erickson, Jacob Shimkus and Grubert, Emily and Hadjimichael, Antonia and Hill, Jason and Mayfield, Erin and Nock, Destenie and Pikok, Kimberly Kivvaq and Saari, Rebecca K. and Samudio Lezcano, Mateo and Siddiqi, Afreen and Skerker, Jennifer B. and Tessum, Christopher W.},
