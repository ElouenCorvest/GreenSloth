  author = {Condon, Dan and Schoene, Blair and Schmitz, Mark and Schaltegger, Urs and Ickert, Ryan B. and Amelin, Yuri and Augland, Lars E. and Chamberlain, Kevin R. and Coleman, Drew S. and Connelly, James N. and Corfu, Fernando and Crowley, James L. and Davies, Joshua H.F.L. and Denyszyn, Steven W. and Eddy, Michael P. and Gaynor, Sean P. and Heaman, Larry M. and Huyskens, Magdalena H. and Kamo, Sandra and Kasbohm, Jennifer and Keller, C. Brenhin and MacLennan, Scott A. and McLean, Noah M. and Noble, Stephen and Ovtcharova, Maria and Paul, Andr{\'e} and Ramezani, Jahandar and Rioux, Matt and Sahy, Diana and Scoates, James S. and Szymanowski, Dawid and Tapster, Simon and Tichomirowa, Marion and Wall, Corey J. and Wotzlaw, J{\"o}rn-Frederik and Yang, Chuan and Yin, Qing-Zhu},
  year = 2024,
  month = sep,
  journal = {Geological Society of America Bulletin},
  volume = {136},
  number = {9-10},
  pages = {4233--4251},
  issn = {0016-7606, 1943-2674},
  doi = {10.1130/B37321.1},
  urldate = {2025-10-29},
  abstract = {Abstract             U-Pb geochronology by isotope dilution--thermal ionization mass spectrometry (ID-TIMS) has the potential to be the most precise and accurate of the deep time chronometers, especially when applied to high-U minerals such as zircon. Continued analytical improvements have made this technique capable of regularly achieving better than 0.1\% precision and accuracy of dates from commonly occurring high-U minerals across a wide range of geological ages and settings. To help maximize the long-term utility of published results, we present and discuss some recommendations for reporting ID-TIMS U-Pb geochronological data and associated metadata in accordance with accepted principles of data management. Further, given that the accuracy of reported ages typically depends on the interpretation applied to a set of individual dates, we discuss strategies for data interpretation. We anticipate that this paper will serve as an instructive guide for geologists who are publishing ID-TIMS U-Pb data, for laboratories generating the data, the wider geoscience community who use such data, and also editors of journals who wish to be informed about community standards. Combined, our recommendations should increase the utility, veracity, versatility, and ``half-life'' of ID-TIMS U-Pb geochronological data.},
  langid = {english},
  file = {/home/elouencorvestnew/Zotero/storage/R45GNUV5/Condon et al. - 2024 - Recommendations for the reporting and interpretation of isotope dilution U-Pb geochronological infor.pdf}
}

