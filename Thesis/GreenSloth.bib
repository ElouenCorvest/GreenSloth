@article{moriartyOverviewPreparationAmerican2024,
  title = {Overview of Preparation for the {{American WAKE ExperimeNt}} ({{AWAKEN}})},
  author = {Moriarty, Patrick and Bodini, Nicola and Letizia, Stefano and Abraham, Aliza and Ashley, Tyler and B{\"a}rfuss, Konrad B. and Barthelmie, Rebecca J. and Brewer, Alan and Brugger, Peter and Feuerle, Thomas and Fr{\`e}re, Ariane and Goldberger, Lexie and Gottschall, Julia and Hamilton, Nicholas and Herges, Thomas and Hirth, Brian and Hung, Lin-Ya (Lilian) and Iungo, Giacomo Valerio and Ivanov, Hristo and Kaul, Colleen and Kern, Stefan and Klein, Petra and Krishnamurthy, Raghavendra and Lampert, Astrid and Lundquist, Julie K. and Morris, Victor R. and Newsom, Rob and Pekour, Mikhail and Pichugina, Yelena and {Port{\'e}-Angel}, Fernando and Pryor, Sara C. and Scholbrock, Andrew and Schroeder, John and Shartzer, Samuel and Simley, Eric and V{\"o}hringer, Lil{\'e}n and Wharton, Sonia and Zalkind, Daniel},
